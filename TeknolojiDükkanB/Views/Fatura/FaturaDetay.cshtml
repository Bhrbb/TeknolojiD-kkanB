@using TeknolojiDükkanB.Models.Sınıflar
@model List<FaturaKalem>
@{
    ViewBag.Title = "FaturaDetay";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Fatura Kalemleri Sayfası </h2>
@*<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active">
        Departman Adı:
        @ViewBag.d
    </li>
    duzenle
</ol>*@

<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Acıklama</th>
        <th>Miktar</th>
        <th>Birim Fiyat</th>
        <th>Toplam Tutar</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.FaturaKalemID</td>
            <td>@x.Aciklama</td>
            <td>@x.Miktar</td>
            <td>@x.BirimFİyat</td>
            <td>@x.Tutar</td>
            
        </tr>
    }

</table>

